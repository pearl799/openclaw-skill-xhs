{
  "_comment": "Add this to your ~/.openclaw/openclaw.json under skills.entries",
  "skills": {
    "entries": {
      "xhs": {
        "env": {
          "XHS_TOOLKIT_DIR": "~/.openclaw/skills/xhs/xhs-toolkit",
          "XHS_COOKIES_FILE": "~/.openclaw/credentials/xhs_cookies.json",
          "XHS_DATA_DIR": "~/.openclaw/skills/xhs/data",
          "XHS_CHROME_PROFILE": "~/.openclaw/skills/xhs/chrome-data",
          "IMAGE_API_KEY": "<YOUR_OPENROUTER_API_KEY>",
          "IMAGE_BASE_URL": "https://openrouter.ai/api/v1/chat/completions",
          "IMAGE_MODEL": "google/gemini-3-pro-image-preview",
          "OPENCLAW_GATEWAY_TOKEN": "<YOUR_GATEWAY_TOKEN>"
        }
      }
    }
  }
}
